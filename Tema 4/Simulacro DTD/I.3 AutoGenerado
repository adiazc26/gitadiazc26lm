<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE partidos [
<!ELEMENT partidos (partido*)>
<!ELEMENT partido (competicion, jornada, local, visitante, fecha, resultado, minutogeneracion, estado, television)>
<!ATTLIST partido
          id ID #REQUIRED>
<!ELEMENT competicion (#PCDATA)>
<!ATTLIST competicion
          año CDATA #REQUIRED>
<!ELEMENT jornada (#PCDATA)>
<!ELEMENT local (#PCDATA)>
<!ATTLIST local
          bandera CDATA #REQUIRED>
<!ELEMENT visitante (#PCDATA)>
<!ATTLIST visitante
          bandera CDATA #REQUIRED>
<!ELEMENT fecha (dia, hora, minuto)>
<!ELEMENT dia (#PCDATA)>
<!ELEMENT hora (#PCDATA)>
<!ELEMENT minuto (#PCDATA)>
<!ELEMENT resultado (#PCDATA)>
<!ATTLIST resultado
          local CDATA #REQUIRED
          visitante CDATA #REQUIRED>
<!ELEMENT minutogeneracion (#PCDATA)>
<!ATTLIST minutogeneracion
          min CDATA #REQUIRED>
<!ELEMENT estado (jugando)>
<!ELEMENT jugando (#PCDATA)>
<!ELEMENT television (#PCDATA)>
<!ATTLIST television
          logo CDATA #REQUIRED>
]>

<partidos>

